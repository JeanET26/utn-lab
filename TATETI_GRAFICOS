	Escribir "                                                                             P A R A    C O N T I N U A R"
	Escribir " "
	Esperar Tecla
	Esperar 1 Segundos
	
	para x<-0 Hasta 8 Con Paso 1 hacer
		vector(x) <- " "		
		
	FinPara
	x<-1;
	
	jugador<- azar(2)+1
	
	Mientras x <= 9 Hacer
		Esperar 1 Segundos
		Borrar Pantalla
		Escribir "                                                                 T U R N O   D E L  J U G A D O R  ", jugador
		Escribir ""
		Escribir ""
		Escribir ""
		Escribir ""
		Escribir ""
		Escribir ""
		Escribir ""
		Escribir "         T A B L E R O   G U I A : "
		Escribir "                                                                             |            |         "
		Escribir "               |       |                                                     |            |         "
		Escribir "            0  |   1   |   2                                        ","    ",vector(0),"    | ", "     ",vector(1),"     |", "     ", vector(2)
		Escribir "          _____|_______|_____                                      __________|____________|__________"
		Escribir "               |       |                                                     |            |"
		Escribir "            3  |   4   |  5                                         ","    ",vector(3),"    | ", "     ",vector(4),"     |", "     ", vector(5)
		Escribir "          _____|_______|_____                                                |            |"
		Escribir "               |       |                                           __________|____________|__________"
		Escribir "            6  |   7   |  8                                                  |            |"
		Escribir "               |       |                                            ","    ",vector(6),"    | ", "     ",vector(7),"     |", "     ", vector(8)
		Escribir "                                                                             |            |"
		Escribir ""
		Escribir ""
		Escribir ""
		Escribir ""
		leer n;
		Escribir ""
		Borrar Pantalla
		Escribir 1 segundos
		
		si (n>=0 y n <10) Entonces
			si (jugador=1) Entonces
				si vector(n)= " " Entonces
					vector(n) <- "X"
					jugador<-2
					
					si vector(0)= "X" y vector(1) = "X" y vector(2) = "X" Entonces
						Esperar 1 Segundos
						Borrar Pantalla
						Escribir ""
